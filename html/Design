        <span className="text-blue-400">‚ñº</span>
        <span>Commodity</span>
      </div>
      <div className="w-32 text-center">Price</div>
      <div className="w-28 text-right">24h %</div>
    </div>

    {/* Commodities List */}
    <div className="px-5 pb-20">
      {commoditiesData.map((commodity) => {
        const showCategory = currentCategory !== commodity.category;
        currentCategory = commodity.category;
        
        return (
          <div key={commodity.rank}>
            {/* Category Header */}
            {showCategory && (
              <div className="py-3 mt-4 mb-2">
                <h3 className="text-lg font-bold text-cyan-400">{commodity.category}</h3>
              </div>
            )}
            
            {/* Commodity Row */}
            <div className="py-4 border-b border-gray-700/30 flex items-center hover:bg-gray-800/20 transition-all rounded-lg px-2 -mx-2">
              <div className="flex items-center gap-3 flex-1 min-w-0">
                <span className="text-gray-400 w-6 text-center font-medium flex-shrink-0">{commodity.rank}</span>
                
                <div 
                  className="w-12 h-12 rounded-full flex items-center justify-center font-bold text-2xl flex-shrink-0 shadow-lg border-2"
                  style={{ 
                    backgroundColor: commodity.color,
                    color: commodity.symbol === 'XAU' || commodity.symbol === 'CORN' ? '#000' : 
                           commodity.symbol === 'XAG' || commodity.symbol === 'XPT' || commodity.symbol === 'XPD' || commodity.symbol === 'AL' || commodity.symbol === 'SUGAR' || commodity.symbol === 'COTTON' ? '#333' : 
                           'white',
                    borderColor: commodity.symbol === 'SUGAR' || commodity.symbol === 'COTTON' ? '#999' : commodity.color,
                    boxShadow: `0 4px 12px ${commodity.color}80`
                  }}
                >
                  {commodity.symbol === 'XAU' && 'ü•á'}
                  {commodity.symbol === 'XAG' && 'ü•à'}
                  {commodity.symbol === 'XPT' && '‚ö™'}
                  {commodity.symbol === 'XPD' && '‚ö´'}
                  {commodity.symbol === 'COPPER' && 'üî∂'}
                  {commodity.symbol === 'AL' && '‚¨ú'}
                  {commodity.symbol === 'NI' && '‚öôÔ∏è'}
                  {commodity.symbol === 'WTI' && 'üõ¢Ô∏è'}
                  {commodity.symbol === 'BRENT' && '‚õΩ'}
                  {commodity.symbol === 'NG' && 'üî•'}
                  {commodity.symbol === 'WHEAT' && 'üåæ'}
                  {commodity.symbol === 'CORN' && 'üåΩ'}
                  {commodity.symbol === 'SOYBEAN' && 'ü´ò'}
                  {commodity.symbol === 'COFFEE' && '‚òï'}
                  {commodity.symbol === 'SUGAR' && 'üç¨'}
                  {commodity.symbol === 'COTTON' && 'üßµ'}
                </div>
                
                <div className="min-w-0">
                  <div className="font-bold text-white text-base truncate">{commodity.name}</div>
                  <div className="text-gray-400 text-xs mt-0.5 truncate uppercase">{commodity.symbol}</div>
                </div>
              </div>

              <div className="w-32 text-center flex-shrink-0">
                <div className="text-white font-semibold text-base">${commodity.price.toFixed(2)}</div>
              </div>

              <div className="w-28 flex flex-col items-end flex-shrink-0">
                <MiniChart change={commodity.change24h} />
                <div className={`text-sm font-bold mt-1.5 flex items-center gap-1 ${
                  commodity.change24h >= 0 ? 'text-emerald-400' : 'text-red-400'
                }`}>
                  <span>{commodity.change24h >= 0 ? '‚ñ≤' : '‚ñº'}</span>
                  <span>{Math.abs(commodity.change24h).toFixed(2)}%</span>
                </div>
              </div>
            </div>
          </div>
        );
      })}
    </div>
  </div>
);
```

}

