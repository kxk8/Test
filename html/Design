import React, { useState } from ‚Äòreact‚Äô;
import { Search, ChevronDown } from ‚Äòlucide-react‚Äô;

const CryptoChartBot = () => {
const [activePage, setActivePage] = useState(‚Äòcoin‚Äô);
const [showTools, setShowTools] = useState(false);
const [showForex, setShowForex] = useState(false);
const [showStocks, setShowStocks] = useState(false);
const [showCommodities, setShowCommodities] = useState(false);
const [scrolled, setScrolled] = useState(false);

React.useEffect(() => {
const handleScroll = (e) => {
const scrollTop = e.target.scrollTop;
setScrolled(scrollTop > 50);
};

```
const container = document.getElementById('main-container');
if (container) {
  container.addEventListener('scroll', handleScroll);
  return () => container.removeEventListener('scroll', handleScroll);
}
```

}, []);

const navigationTabs = [
{ id: ‚Äòcoin‚Äô, label: ‚ÄòCoin‚Äô },
{ id: ‚Äòtools‚Äô, label: ‚ÄòTools‚Äô },
{ id: ‚Äòfavourite‚Äô, label: ‚ÄòFav‚Äô },
{ id: ‚Äòforex‚Äô, label: ‚ÄòForex‚Äô },
{ id: ‚Äòcommodities‚Äô, label: ‚ÄòCommodities‚Äô },
{ id: ‚Äònft‚Äô, label: ‚ÄòNFT‚Äô },
{ id: ‚Äòstocks‚Äô, label: ‚ÄòStocks‚Äô },
{ id: ‚Äòbonds‚Äô, label: ‚ÄòBonds‚Äô }
];

const handleTabClick = (pageId) => {
setActivePage(pageId);
if (pageId === ‚Äòtools‚Äô) {
setShowTools(true);
setShowForex(false);
setShowStocks(false);
setShowCommodities(false);
} else if (pageId === ‚Äòforex‚Äô) {
setShowForex(true);
setShowTools(false);
setShowStocks(false);
setShowCommodities(false);
} else if (pageId === ‚Äòstocks‚Äô) {
setShowStocks(true);
setShowTools(false);
setShowForex(false);
setShowCommodities(false);
} else if (pageId === ‚Äòcommodities‚Äô) {
setShowCommodities(true);
setShowTools(false);
setShowForex(false);
setShowStocks(false);
} else {
setShowTools(false);
setShowForex(false);
setShowStocks(false);
setShowCommodities(false);
}
};

